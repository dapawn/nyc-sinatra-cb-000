<h1>NYC Figures</h1>
  <table>
  <th>Person Name:</th>
  <% @figures.each do |figure| %>
  <tr>
      <td><a href="/figures/<%= figure.id %>"><%= figure.name %></a>
      <ul>
        <%figure.titles.each do |title|%>
        <li><%=title.name%></li>
        <%end%>
      </ul>
     </tr> </td>
  <% end %>
</table>
<br>
<a class="button" href="/">Home</a>
